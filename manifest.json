{
  "description": "Tabless Suspenders",
  "browser_specific_settings": {
    "gecko": {
      "id": "tabless@stebalien.com"
    }
  },
  "homepage_url": "https://github.com/Stebalien/tabless",
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "sleep.png",
    "default_title": "Tabless Suspenders",
    "browser_style": true,
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "<all_urls>", "sessions", "history", "webNavigation"],
  "name": "Tabless Suspenders",
  "version": "1.0"
}
