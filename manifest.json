{
  "description": "Tabless Browsing",
  "applications": {
    "gecko": {
      "id": "tabless@stebalien.com",
      "strict_min_version": "54.0"
    }
  },
  "homepage_url": "http://stebalien.com/projects/tabless",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["unload.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "sleep.png",
    "default_title": "Suspend Tabs"
  },
  "permissions": ["tabs", "<all_urls>"],
  "name": "Tabless",
  "version": "1.0"
}
